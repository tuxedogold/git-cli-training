
[alias]
    ac = !git add . && git commit -m
	up = pull origin master
	p = !git push
    b = !git checkout -b
	co = !git checkout
    s = !git status
    m = !git checkout master
    p = !git push
	uncommit = reset --soft HEAD~1
	unstage = reset HEAD --
	pre = pull origin master --rebase
	up = pull origin master
	amend = commit -a --amend
	branchname = branch --show-current
